{
  "name": "@movingco/core",
  "version": "0.4.0",
  "description": "Core packages for MovingKit.",
  "author": "Ian Macalinao <ian@moving.wtf>",
  "homepage": "https://github.com/movingco/moving-kit#readme",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
